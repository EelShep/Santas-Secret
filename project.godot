AudioController="*res://src/globals/audio_controller/audio_controller.tscn"
GameData="*res://src/globals/game_data.gd"
SceneTransition="*res://src/globals/scene_transition/scene_transition.tscn"
MetSys="*res://addons/MetroidvaniaSystem/Nodes/Singleton.tscn"
