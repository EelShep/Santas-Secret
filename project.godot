config_version=5

[addons]

metroidvania_system/settings_file="res://MetSysSettings.tres"

[application]

config/name="Github GameOff"
run/main_scene="res://scenes/main/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://src/resources/default_bus_layout.tres"

[autoload]

