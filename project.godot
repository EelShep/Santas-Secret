[display]

window/size/viewport_width=1024
window/size/viewport_height=576
window/size/always_on_top=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/MetroidvaniaSystem/EditorExtension/plugin.cfg", "res://addons/MetroidvaniaSystem/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

